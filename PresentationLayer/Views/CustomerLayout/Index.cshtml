@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">
@await Component.InvokeAsync("_CustomerLayoutHeaderPartial")
<body class="nav-md">
    <div class="container body">
        <div class="main container">
            @await Component.InvokeAsync("_CustomerLayoutSidebarPartial")
            @await Component.InvokeAsync("_CustomerLayoutNavbarPartial")
            <div class="" role="main">
                @RenderBody
            </div>
            @await Component.InvokeAsync("_CustomerLayoutFooterPartial")
                
        </div>
    </div>
    <script src="~/gentela-gh-pages/vendors/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/gentela-gh-pages/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/gentela-gh-pages/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/gentela-gh-pages/vendors/nprogress/nprogress.js"></script>
    <!-- bootstrap-wysiwyg -->
    <script src="~/gentela-gh-pages/vendors/bootstrap-wysiwyg/js/bootstrap-wysiwyg.min.js"></script>
    <script src="~/gentela-gh-pages/vendors/jquery.hotkeys/jquery.hotkeys.js"></script>
    <script src="~/gentela-gh-pages/vendors/google-code-prettify/src/prettify.js"></script>

    <!-- Custom Theme Scripts -->
</body>

</html>
